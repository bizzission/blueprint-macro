{
    "name": "cleaniquecoders/blueprint-macro",
    "description": "Laravel Blueprint Macro",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Blueprint\\Macro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Blueprint\\Macro\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^5.5|^5.6|^5.7",
        "illuminate/support": "^5.5|^5.6|^5.7",
        "illuminate/auth": "^5.5|^5.6|^5.7",
        "doctrine/dbal": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*",
        "mockery/mockery": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\Blueprint\\Macro\\BlueprintMacroServiceProvider"
            ]
        }
    }
}
